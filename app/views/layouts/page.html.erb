<!DOCTYPE html>
<html>
  <head>
    <title>National Book Store</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'true' %>
  </head>

  <body>
	  <!-- <div class="container-fluid"> -->
    <%= render "common/navbar" %>
    <%= render "common/flashes" %>

      <div class="wrapper product-wrap">
        <div class="container">

          <div class="row">
      			<div class="col-md-12">
              <ol class="breadcrumb breadcrumb-jd">
                <li><a href="/">Home</a></li>
                <li><strong>服务中心</strong></li>
              </ol>
      			</div>
      		</div>

          <div class="row">
            <div class="col-md-12"></div>

              <div class="col-md-2">
                <%= render "common/left_nav_about" %>
        			</div>

        			<div class="col-md-10">
                <div class="row">
                  <div class="col-md-12">
                    <%= yield %>
                  </div>


                </div>
            </div>  <!-- col-md-10 end -->

          </div>     <!-- row end -->


        </div>
      </div>


	  <!-- </div> -->
	  <%= render "common/footer" %>
  </body>
</html>
